{
  "name": "courtside-mobile-app",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "lint": "npx expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:dev": "npx eas build --profile development",
    "build:preview": "npx eas build --profile preview",
    "build:prod": "npx eas build --profile production",
    "submit:ios": "npx eas submit --platform ios --latest",
    "submit:android": "npx eas submit --platform android --latest",
    "submit:all": "npx eas submit --platform all --latest",
    "deploy:firebase": "npx firebase deploy",
    "deploy:rules": "npx firebase deploy --only firestore:rules",
    "deploy:functions": "npx firebase deploy --only functions",
    "seed:test-data": "npx ts-node --project scripts/tsconfig.json scripts/seed-test-data.ts",
    "verify:data": "npx ts-node --project scripts/tsconfig.json scripts/verify-data.ts",
    "clear:test-tournaments": "npx ts-node --project scripts/tsconfig.json scripts/clear-test-tournaments.ts",
    "cleanup-hangry-joes": "npx ts-node --project scripts/tsconfig.json scripts/cleanup-keep-hangry-joes.ts",
    "type-check": "npx tsc --noEmit",
    "security-audit": "npm audit --audit-level=high"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/material-top-tabs": "^7.4.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.4.10",
    "date-fns": "^4.1.0",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.9",
    "firebase": "^12.4.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^4.2.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/node": "^24.10.1",
    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.7",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "firebase-admin": "^13.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "^54.0.13",
    "lodash": "^4.17.21",
    "react-test-renderer": "19.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
